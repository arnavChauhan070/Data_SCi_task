   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Highest Attendance: 95\n",
      "Month with Highest Attendance: Mar\n",
      "Monthly Changes: [ 5  5 -7]\n",
      "Trend Line Equation: y = 1.40x + 87.40\n"
     ]
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 640x480 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# Data\n",
    "months = [\"Jan\", \"Feb\", \"Mar\", \"Apr\"]\n",
    "attendance = [85, 90, 95, 88]\n",
    "\n",
    "max_attendance = max(attendance)\n",
    "max_month = months[attendance.index(max_attendance)]\n",
    "\n",
    "print(\"Highest Attendance:\", max_attendance)\n",
    "print(\"Month with Highest Attendance:\", max_month)\n",
    "\n",
    "differences = np.diff(attendance)\n",
    "print(\"Monthly Changes:\", differences)\n",
    "\n",
    "x = np.arange(len(attendance))   # numeric index for months\n",
    "coefficients = np.polyfit(x, attendance, 1)  # degree 1 = straight line\n",
    "trend_line = np.poly1d(coefficients)\n",
    "\n",
    "print(\"Trend Line Equation: y = {:.2f}x + {:.2f}\".format(coefficients[0], coefficients[1]))\n",
    "\n",
    "\n",
    "plt.plot(months, attendance, marker='o', label=\"Attendance\")\n",
    "plt.plot(months, trend_line(x), linestyle='--', label=\"Trend Line\")\n",
    "\n",
    "plt.title(\"Monthly Attendance Track Record\")\n",
    "plt.xlabel(\"Months\")\n",
    "plt.ylabel(\"Attendance\")\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "\n",
    "plt.show()"
   ]
  },
