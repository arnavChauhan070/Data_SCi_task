     "text": [
      "The systolic values are:\n",
      " [120 134 140 125]\n",
      "Ptients with systolic > 130:\n",
      " [[134  85]\n",
      " [140  90]]\n",
      "Replace:\n",
      " [[120  80]\n",
      " [134  85]\n",
      " [140  90]\n",
      " [125  80]]\n"
     ]
    }
   ],
   "source": [
    "# Health Care Dataset\n",
    "import numpy as np\n",
    "bp = np.array([[120,80],\n",
    "                 [134,85],\n",
    "                 [140,90],\n",
    "                 [125,75]])\n",
    "# Extract systolic values\n",
    "systolic_values = bp[:, 0]\n",
    "print(\"The systolic values are:\\n\", systolic_values)\n",
    "# Patients with systolic > 130 using boolean indexing\n",
    "high_values = bp[bp[:, 0] > 130]\n",
    "print(\"Ptients with systolic > 130:\\n\", high_values)\n",
    "# Replace all < 80 with 80\n",
    "bp[bp<80] = 80\n",
    "print(\"Replace:\\n\", bp)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 135,
   "id": "6b281bc2-e0a8-4de9-872c-943b83f9f46e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
