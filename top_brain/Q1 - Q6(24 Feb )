   "source": [
    "# Retail Sales analysis\n",
    "import numpy as np\n",
    "sales_day1 = np.array([120, 89, 90])\n",
    "sales_day2 = np.array([150, 90, 75])\n",
    "\n",
    "# Total sales per product across 2 days\n",
    "total_sales = sales_day1 + sales_day2\n",
    "print(\"Total sales per product:\", total_sales)\n",
    "\n",
    "# Total sales each day\n",
    "day1_sale = np.sum(sales_day1)\n",
    "day2_sale = np.sum(sales_day2)\n",
    "\n",
    "# Percentage growth from day1 to day2\n",
    "percent_growth = ((day2_sale - day1_sale) / day1_sale) * 100\n",
    "print(\"Overall percentage growth:\", percent_growth)\n",
    "\n",
    "# Product with highest growth\n",
    "product_growth = sales_day2 - sales_day1\n",
    "highest_growth_product = np.argmax(product_growth)\n",
    "print(\"Product with highest growth index:\", highest_growth_product)"
   ]
  },
