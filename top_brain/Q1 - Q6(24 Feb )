   "source": [
    "# Weather Data(Broadcasting)\n",
    "import numpy as np\n",
    "temps = np.array([[25,23,45],\n",
    "                 [34,67,86],\n",
    "                 [32,12,25],\n",
    "                 [20,21,23]])\n",
    "# Convert all temps in fahrenheit using broadcasting\n",
    "Fahrenheit = ((temps * 9/5) + 32)\n",
    "print(\"Fahrenheit temps are:\\n\", Fahrenheit)\n",
    "# Add a correction factor [1, -1, 0] to adjust readings for sensor bias.\n",
    "correction = np.array([1, -1, 0])\n",
    "adjusted_temps = temps + correction\n",
    "print(\"\\nAdjusted Celsius temps:\\n\", adjusted_temps)\n",
    "# If you also want adjusted Fahrenheit:\n",
    "adjusted_fahrenheit = (adjusted_temps * 9/5) + 32\n",
    "print(\"\\nAdjusted Fahrenheit temps:\\n\", adjusted_fahrenheit)"
   ]
  },
