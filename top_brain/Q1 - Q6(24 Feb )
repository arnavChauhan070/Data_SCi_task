    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "regions = [\"North\", \"South\", \"East\", \"West\"]\n",
    "books = [1000, 1500, 1700, 6000]\n",
    "\n",
    "highest_activity = max(books)\n",
    "high_activity_region = regions[books.index(highest_activity)]\n",
    "\n",
    "print(\"Highest checkout books:\", highest_activity)\n",
    "print(\"Region with Highest Checkout:\", high_activity_region)\n",
    "\n",
    "lowest_activity = min(books)\n",
    "low_activity_region = regions[books.index(lowest_activity)]\n",
    "\n",
    "print(\"Lowest checkout books:\", lowest_activity)\n",
    "print(\"Region with Lowest Checkout:\", low_activity_region)\n",
    "\n",
    "x = np.arange(len(books))   # FIXED (was arrange)\n",
    "coefficients = np.polyfit(x, books, 1)\n",
    "trend_line = np.poly1d(coefficients)\n",
    "\n",
    "print(\"Trend Line Equation: y = {:.2f}x + {:.2f}\".format(coefficients[0], coefficients[1]))\n",
    "\n",
    "plt.bar(regions, books, label=\"Books\")\n",
    "\n",
    "plt.plot(regions, trend_line(x), linestyle='--', label=\"Trend Line\")\n",
